@model DAL.ViewModels.EditStudentParrentViewModel

@{
    ViewData["Title"] = "ویرایش اطلاعات";
}


<h4></h4>
<hr />
<div class="row">
    <div class="col s12 m4 offset-m4">
        <div class="card white lighten-1">
            <br />
            <form asp-action="EditProfile">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="Name" class="control-label"></label>
                    <input asp-for="Name" class="form-control" />
                    <span asp-validation-for="Name" class="text-danger"></span>
                </div>

                <div class="form-group">
                    <label asp-for="TelNumber" class="control-label"></label>
                    <input asp-for="TelNumber" class="form-control" />
                    <span asp-validation-for="TelNumber" class="text-danger"></span>
                </div>

                <div class="form-group">
                    <p class="center">
                        <input type="submit" value="ثبت  نهایی" class="btn btn-primary" />

                    </p>
                    <br />
                </div>
            </form>
        </div>
    </div>
</div>


