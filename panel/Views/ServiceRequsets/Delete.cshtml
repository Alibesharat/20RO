@model DAL.ServiceRequset

@{
    ViewData["Title"] = "حذف";
}


<h4>آیا شما از حذف این آیتم اطمینان دارید ؟</h4>
<div>
     <hr />
    <p>
        @Html.DisplayNameFor(model => model.studentParent) :   @Html.DisplayFor(model => model.studentParent.Id)
    </p> 
    <p>
        @Html.DisplayNameFor(model => model.FullName) : @Html.DisplayFor(model => model.FullName)
    </p>
    <p>
        @Html.DisplayNameFor(model => model.gender) : @Html.DisplayFor(model => model.gender)
    </p>
    <p>
        @Html.DisplayNameFor(model => model.Age) : @Html.DisplayFor(model => model.Age)
    </p>
    <p>
        @Html.DisplayNameFor(model => model.Address) : @Html.DisplayFor(model => model.Address)
    </p>
    <p>
        @Html.DisplayNameFor(model => model.Note) : @Html.DisplayFor(model => model.Note)
    </p>
    <p>
        @Html.DisplayNameFor(model => model.course) :   @Html.DisplayFor(model => model.course.Id)
    </p> 
    <p>
        @Html.DisplayNameFor(model => model.RequsetState) : @Html.DisplayFor(model => model.RequsetState)
    </p>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <a asp-action="Index" class="btn btn-default">خیر لغو شود</a> 
        <input type="submit" value="بله اطمینان دارم این آیتم  حذف شود " class="btn btn-danger" /> 
    </form>
</div>
